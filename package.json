{
  "name": "client",
  "version": "0.0.1",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --config video=false screenshotOnRunFailure=false",
    "ng": "ng",
    "start:node": "node server.js",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:docker": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "test:coverage": "ng test --codeCoverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "start-server-and-test start http://localhost:4200 cy:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@babel/compat-data": "^7.8.0",
    "@briebug/cypress-schematic": "^4.0.3",
    "@stomp/ng2-stompjs": "^7.2.0",
    "express": "^4.17.1",
    "ng-zorro-antd": "^10.0.0",
    "path": "^0.12.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.4",
